{
  "name": "ratelock",
  "private": true,
  "description": "RateLock is a rate limiting library for Node.js",
  "type": "module",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format:check": "prettier \"**/*\" --ignore-unknown --list-different",
    "format:fix": "prettier \"**/*\" --ignore-unknown --list-different --write",
    "clean": "turbo clean && rimraf node_modules",
    "version": "changeset version",
    "release": "changeset publish",
    "typecheck": "turbo typecheck",
    "prepare": "husky",
    "pre-commit": "turbo pre-commit --concurrency=1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/types": "^19.8.1",
    "@ratelock/eslint": "workspace:*",
    "@ratelock/prettier": "workspace:*",
    "@ratelock/tsconfig": "workspace:*",
    "@types/eslint": "^9.6.1",
    "@types/node": "^24.0.13",
    "eslint": "^9.30.1",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "rimraf": "^6.0.1",
    "turbo": "^2.5.5",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@10.13.1",
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/saoudi-h/ratelock.git"
  },
  "keywords": [
    "rate-limiting",
    "rate-locking",
    "rate-lock",
    "ratelimit",
    "redis",
    "nextjs",
    "express",
    "fastify",
    "rate-limiter",
    "nestjs",
    "middleware",
    "security",
    "typescript"
  ],
  "author": {
    "name": "Saoudi Hakim",
    "url": "https://hakimsaoudi.dev",
    "email": "saoudihakim@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.10"
  }
}
