{
    "name": "@ratelock/redis",
    "version": "0.1.1",
    "description": "Redis storage backend for RateLock rate limiting system",
    "keywords": [
        "rate-limit",
        "rate-limiting",
        "redis",
        "throttle",
        "api-rate-limit",
        "sliding-window",
        "fixed-window",
        "token-bucket"
    ],
    "homepage": "https://github.com/saoudi-h/ratelock",
    "bugs": {
        "url": "https://github.com/saoudi-h/ratelock/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/saoudi-h/ratelock.git",
        "directory": "packages/redis"
    },
    "license": "MIT",
    "author": {
        "name": "Saoudi Hakim",
        "url": "https://hakimsaoudi.dev",
        "email": "saoudihakim@gmail.com"
    },
    "type": "module",
    "main": "./dist/index.cjs",
    "types": "./dist/index.d.cts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        },
        "./factory": {
            "import": "./dist/factory/index.mjs",
            "require": "./dist/factory/index.cjs"
        },
        "./storage": {
            "import": "./dist/storage/index.mjs",
            "require": "./dist/storage/index.cjs"
        },
        "./strategy": {
            "import": "./dist/strategy/index.mjs",
            "require": "./dist/strategy/index.cjs"
        },
        "./package.json": "./package.json"
    },
    "sideEffects": false,
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build:lua": "tsx scripts/lua-minifier.ts",
        "build": "pnpm build:lua && pnpm tsdown",
        "dev": "tsdown --watch",
        "test": "vitest --exclude \"**/*.integration.test.ts\"",
        "test:ui": "vitest --exclude \"**/*.integration.test.ts\" --ui",
        "test:run": "vitest run --exclude \"**/*.integration.test.ts\"",
        "test:coverage": "vitest run --coverage",
        "test:coverage:dev": "pnpm run docker:up && vitest run --coverage && pnpm run docker:down",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "typecheck": "tsc --noEmit",
        "clean": "rm -rf dist .turbo node_modules",
        "prepack": "tsdown",
        "docker:up": "docker-compose --env-file .env.test -f docker-compose.test.yml up -d --wait",
        "docker:down": "docker-compose --env-file .env.test -f docker-compose.test.yml down",
        "test:integration": "pnpm run docker:up && vitest run && pnpm run docker:down",
        "pre-commit": "lint-staged"
    },
    "peerDependencies": {
        "redis": "^4.0.0 || ^5.0.0"
    },
    "dependencies": {
        "@ratelock/core": "workspace:*"
    },
    "devDependencies": {
        "@ratelock/core": "workspace:*",
        "@ratelock/eslint": "workspace:*",
        "@ratelock/prettier": "workspace:*",
        "@ratelock/test-utils": "workspace:*",
        "@ratelock/tsconfig": "workspace:*",
        "@ratelock/tsdown": "workspace:*",
        "@ratelock/vitest": "workspace:*",
        "@types/node": "^24.3.0",
        "dotenv": "^17.2.1",
        "luamin": "^1.0.4",
        "typescript": "^5.5.4"
    },
    "engines": {
        "node": ">=18"
    },
    "publishConfig": {
        "access": "public"
    },
    "module": "./dist/index.mjs"
}
