{
  "name": "@ratelock/core",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./limiter": {
      "import": "./dist/limiter/index.mjs",
      "require": "./dist/limiter/index.cjs"
    },
    "./storage": {
      "import": "./dist/storage/index.mjs",
      "require": "./dist/storage/index.cjs"
    },
    "./strategy": {
      "import": "./dist/strategy/index.mjs",
      "require": "./dist/strategy/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --build tsconfig.build.json",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --max-warnings 0 --fix",
    "format:check": "prettier \"**/*\" --ignore-unknown --list-different",
    "format:fix": "prettier \"**/*\" --ignore-unknown --write",
    "clean": "rimraf node_modules dist",
    "typecheck": "tsc"
  },
  "devDependencies": {
    "@ratelock/eslint": "workspace:*",
    "@ratelock/prettier": "workspace:*",
    "@ratelock/tsconfig": "workspace:*",
    "@types/node": "^24.0.13",
    "eslint": "^9.30.1",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.2",
    "vite-plugin-static-copy": "^3.1.0"
  },
  "keywords": [
    "core",
    "rate-locking",
    "rate-lock",
    "ratelimit",
    "redis",
    "nextjs",
    "express",
    "fastify",
    "rate-limiter",
    "nestjs",
    "middleware",
    "security",
    "typescript"
  ],
  "author": {
    "name": "Saoudi Hakim",
    "url": "https://hakimsaoudi.dev",
    "email": "saoudihakim@gmail.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "module": "./dist/index.mjs"
}
